              SQLTask:SqlStatementSource="DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@eventId int = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@userEmail varchar(100) = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@sysDate datetime = DATEADD(DAY,1,SYSDATETIME())&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@kickoffDate datetime = DATETIMEFROMPARTS(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Year(@sysDate),&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Month(@sysDate),&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Day(@sysDate),&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;23,0,0,0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;);&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@jsonObjLen int = 36; -- allows 5 characters above typical object size + declared variables&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@jsonObjMax int = 50; --CASE WHEN (2000 / @jsonObjLen) &gt; 50 THEN 50 ELSE 2000 / @jsonObjLen END; -- 2000 is max character length for the DB field storing the JSON body for Deputy&#xA;&#xA;-- Create Event to update Deputy&#xA;DECLARE @event TABLE (eventId int);&#xA;DECLARE @impeventId int = -1;&#xA;&#xA;INSERT INTO eda.ETL_EVENT (ETL_REF, ETL_RUN_CODE, EVENT_STATUS, EVENT_DATE_CREATE, EVENT_SOURCE, EVENT_INPUT, EVENT_OUTPUT, EVENT_USER_EMAIL, DATE_LAST_MODIFIED, EVENT_KICKOFF_DATE)&#xA;OUTPUT Inserted.EVENT_ID INTO @event&#xA;VALUES (&#xA;&#x9;6002,&#xA;&#x9;0,&#xA;&#x9;'NEW',&#xA;&#x9;GETDATE(),&#xA;&#x9;'Payroll Extract', -- Source of the Event&#xA;&#x9;'TIMESHEET PAID', --DATA SOURCE options: Timesheet Paid, Employee Custom Field, General Resource, Emp Agree Migration, Timesheet Reprocess, Deputy&#xA;&#x9;'DEPUTY', -- DATA TARGET options: Preceda, Billing, Subcontractor, Deputy&#xA;&#x9;@userEmail, -- Use your own email here&#xA;&#x9;GETDATE(),&#xA;&#x9;@kickoffDate -- NULL if you want the event to process now, otherwise provide datetime it should run server time (perth - UTC + 8)&#xA;);&#xA;&#xA;SELECT @impeventId = eventId FROM @event;&#xA;&#xA;&#xA;WITH&#x9;&#x9;&#x9;&#x9;&#x9;payExtracted AS(&#xA;SELECT DISTINCT&#x9;&#x9;&#x9;t.Id timeSheetID,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;p.PeriodId,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ROW_NUMBER() OVER (PARTITION BY p.periodId ORDER BY p.periodId, t.Id) rowNum&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL t&#xA;LEFT JOIN&#x9;&#x9;&#x9;&#x9;eda.STG_DEP_EmpeePaycycle p ON t.PayCycleId = p.Id&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;t.MDEventId = @eventId&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;t.TimeApproved = 1&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ISNULL(t.LeaveRuleId,0) NOT IN (11,27,28,29,30,31)&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;t.payCentre IN ('Employee Timesheets', 'Agency Timesheets')&#xA;AND &#x9;&#x9;&#x9;&#x9;&#x9;(isnull(t.timesheetHours,0) + isnull(t.phHours,0)) &gt; 0.12),&#xA;Grouped AS (&#xA;SELECT&#x9;&#x9;&#x9;&#x9;&#x9;((rowNum - 1) / @jsonObjMax) AS grp,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;timeSheetID,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PeriodId&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;payExtracted&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;PeriodId is NOT NULL&#xA;&#xA;&#xA;)&#xA;INSERT INTO&#x9;&#x9;&#x9;&#x9;eda.IMP_DEPRESOURCE (eventId,apiType,resource,recordId,isBulk,isInsert,reqBody)&#xA;SELECT&#x9;&#x9;&#x9;&#x9;&#x9;@impeventId,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'na',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'paid',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PeriodId,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;0,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;0,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CONCAT('{&quot;TimesheetIdArray&quot;:[',STRING_AGG(timeSheetID,','),']}') JsonBody&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;Grouped&#xA;GROUP BY&#x9;&#x9;&#x9;&#x9;grp, PeriodId&#xA;ORDER BY&#x9;&#x9;&#x9;&#x9;grp, PeriodId;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
