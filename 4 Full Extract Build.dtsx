      <DTS:VariableValue
        DTS:DataType="8">DECLARE			@skPHDate int = 226
DECLARE			@isNational bit = 0 
DECLARE			@mainDate date = '2025-1-20'
DECLARE			@altDate date = '1900-1-1'
DECLARE 			@maxDays int = 13 
DECLARE 			@passDays int = 7 
DECLARE			@phRef int =-1


IF @isNational=0 SELECT @phRef = hd.FK_Holiday FROM dw.DIM_PUBLIC_HOLIDAY_DATES hd WHERE hd.SK_DIM_PUBLIC_HOLIDAY_DATES = @skPHDate;


DECLARE @southCutover date = '2024-11-11'
DECLARE @northCutover date = '2024-11-25'

DECLARE @mainFromDate date = DATEADD(WW,-1 * @maxDays,@mainDate)
DECLARE @altFromDate date = DATEADD(WW,-1 * @maxDays,@altDate)



/** Working Day Build **/
SELECT *  FROM (
--SELECT		coalesce(mQry.employeeId, aQry.employeeId, mStat.employeeId, aStat.employeeId, mWorked.employeeId, aWorked.employeeId) employeeId,
--			coalesce(mQry.employeeRef, aQry.employeeRef, mStat.employeeRef, aStat.employeeRef, mWorked.employeeRef, aWorked.employeeRef) employeeRef,
--			coalesce(mQry.employeeName, aQry.employeeName, mStat.employeeName, aStat.employeeName, mWorked.employeeName, aWorked.employeeName) employeeName,
SELECT		coalesce(mQry.employeeId, aQry.employeeId, mStat.employeeId, aStat.employeeId, empTbl.employeeId) employeeId,
			coalesce(mQry.employeeRef, aQry.employeeRef, mStat.employeeRef, aStat.employeeRef, empTbl.employeeRef) employeeRef,
			coalesce(mQry.employeeName, aQry.employeeName, mStat.employeeName, aStat.employeeName, empTbl.employeeName) employeeName,
			coalesce(CASE
				WHEN mQry.numWeeks = @maxDays AND mQry.shiftCount &gt;= @passDays THEN 1
				WHEN mQry.numWeeks &lt; @maxDays AND mQry.shiftCount / CASE WHEN mQry.numWeeks = 0 THEN 1 ELSE mQry.numWeeks END &gt;= 0.5 THEN 1
				ELSE 0
			END,0) mainWorkingDay,
			coalesce(CASE
				WHEN aQry.numWeeks = @maxDays AND aQry.shiftCount &gt;= @passDays THEN 1
				WHEN aQry.numWeeks &lt; @maxDays AND aQry.shiftCount / CASE WHEN aQry.numWeeks = 0 THEN 1 ELSE aQry.numWeeks END &gt;= 0.5 THEN 1
				ELSE 0
			END,0) altWorkingDay,
			isnull(mStat.StatTaken,0) mainStatHolidayTaken,
			isnull(aStat.StatTaken,0) altStatHolidayTaken,
			isnull(round(mQry.avHours * 4,0)/4,0) mainAvHours,
			isnull(round(aQry.avHours * 4,0)/4,0) altAvHours,
			coalesce(cast(e.P_Region as varchar(50)), mQry.branchName, aQry.branchName, empTbl.branchName) region,
			bph.fk_dim_publicholiday
			--isnull(mWorked.hoursWorked,0) mainHrsWorked,
			--isnull(aWorked.hoursWorked,0) altHrsWorked
FROM (

SELECT		deputy.employeeId,
			isnull(deputy.employeeRef, intime.employeeRef) employeeRef,
			isnull(deputy.employeeName, intime.employeeName) employeeName,
			isnull(intime.shiftCount,0) + isnull(deputy.shiftCount,0) shiftCount,
			CASE WHEN isnull(intime.numWeeks,0) &gt; isnull(deputy.numWeeks,0) THEN intime.numWeeks else deputy.numWeeks END numWeeks,
			deputy.avHours,
			deputy.branchName
FROM		(

SELECT		NULL employeeId,
			cast(g.PK_SourceIdStr as varchar(50)) employeeRef,
			g.FirstName + ' ' + g.LastName employeeName ,
			cast(
				COUNT(DISTINCT 
				CASE
					WHEN DATEPART(DW,cast(h.RosterFrom as date)) = DATEPART(DW,@mainDate) THEN cast(h.rosterTo as date)
					ELSE d.PK_Date
				END
				) 
				as float) shiftCount,
			cast(DATEDIFF(DD, min(d.PK_Date),@mainDate)/7 as float) numWeeks
FROM		dw.FACT_ROSTER h
INNER JOIN	dw.DIM_SECURITYGUARD g ON h.FK_DIM_SECURITYGUARD = g.SK_DIM_SECURITYGUARD
INNER JOIN	dw.DIM_DATE d ON h.FK_DIM_DATE = d.SK_DIM_DATE
INNER JOIN	dw.DIM_IT_STAFF_BRANCH sb ON g.SK_DIM_SECURITYGUARD = sb.FK_DIM_SECURITYGUARD AND isnull(sb.P_ExpiryDate,'2999-12-31') &gt;= CASE WHEN @mainDate &gt; @altDate THEN @mainDate ELSE @altDate END
INNER JOIN	dw.DIM_DEPOT dp ON sb.FK_DIM_DEPOT = dp.SK_DIM_DEPOT
LEFT JOIN	dw.DIM_BRANCH b on dp.FK_DIM_BRANCH = b.SK_DIM_BRANCH
WHERE		((h.RosterFrom &gt;= @mainFromDate AND h.RosterTo &lt; @mainDate AND DATEPART(DW,h.RosterFrom) = DATEPART(DW,@mainDate))
OR			(h.RosterTo &gt;= @mainFromDate AND h.RosterTo &lt; @mainDate AND DATEPART(DW,h.RosteRTo) = DATEPART(DW,@mainDate)))
AND			((b.[P_Branch Group] = 'Southern Region' AND h.rosterFrom &lt; @southCutover) OR (b.[P_Branch Group] != 'Southern Region' AND h.rosterTo &lt; @northCutover))
GROUP BY	g.PK_SourceIdStr, g.FirstName + ' ' + g.LastName) intime

FULL OUTER JOIN (

SELECT			e.employeeId,
				ea.payrollId employeeRef,
				e.displayName employeeName,
				cast(
				COUNT(DISTINCT 
				CASE
					WHEN DATEPART(DW,cast(isnull(t.timesheetEnd, r.RosterEnd) as date)) = DATEPART(DW,@mainDate) THEN cast(isnull(t.timesheetEnd, r.RosterEnd) as date)
					ELSE d.PK_Date
				END
				) 
				as float) shiftCount,
				cast(DATEDIFF(DD, min(d.PK_Date),@mainDate)/7 as float) numWeeks,
				AVG(isnull(t.timesheetHours, r.Hours)) avHours,
				l1.Name branchName
FROM			staging.FCT_DEP_ROSTER r
FULL OUTER JOIN staging.FCT_DEP_Timesheet t ON r.Id = t.RosterId
INNER JOIN		dw.DIM_DATE d on isnull(cast(t.timesheetStart as date),r.RosterDate) = d.PK_Date
LEFT JOIN		staging.DEP_EmployeeAgreement ea On isnull(t.EmployeeId, r.EmployeeId) = ea.employeeId
INNER JOIN		staging.DEP_Employee e ON isnull(t.EmployeeId, r.EmployeeId) = e.employeeId
INNER JOIN		staging.DEP_EmployeeBranchLocation ebl ON e.employeeId = ebl.Id
LEFT JOIN		staging.DEP_Location l1 ON ebl.Company = l1.Id 
LEFT JOIN		staging.DEP_Location l2 ON l1.ParentId = l2.Id 
LEFT JOIN		staging.DEP_Location l3 ON l2.ParentId = l3.Id 
WHERE			((isnull(t.timesheetStart,r.RosterStart) &gt;= @mainFromDate AND isnull(t.timesheetStart,r.RosterStart) &lt; @mainDate AND DATEPART(DW,isnull(t.timesheetStart,r.RosterStart)) = DATEPART(DW,@mainDate))
	OR				(isnull(t.timesheetEnd,r.RosterEnd) &gt;= @mainFromDate AND isnull(t.timesheetEnd,r.RosterEnd) &lt; @mainDate AND DATEPART(DW,isnull(t.timesheetEnd,r.RosterEnd)) = DATEPART(DW,@mainDate)))
AND				((CASE WHEN l3.name like '%region' THEN l3.name WHEN l2.name like'%region' THEN l2.name ELSE l1.name END = 'South Region' AND isnull(t.timesheetStart,r.RosterStart) &gt;= @southCutover)
	OR				(CASE WHEN l3.name like '%region' THEN l3.name WHEN l2.name like'%region' THEN l2.name ELSE l1.name END != 'South Region' AND isnull(t.timesheetStart,r.RosterStart) &gt;= @northCutover))
AND				t.Discarded = 0

GROUP BY		e.employeeId, ea.payrollId, e.displayName, l1.Name
				) deputy ON intime.employeeRef = deputy.employeeRef

				) mQry

FULL OUTER JOIN (

SELECT		deputy.employeeId,
			isnull(deputy.employeeRef, intime.employeeRef) employeeRef,
			isnull(deputy.employeeName, intime.employeeName) employeeName,
			isnull(intime.shiftCount,0) + isnull(deputy.shiftCount,0) shiftCount,
			CASE WHEN isnull(intime.numWeeks,0) &gt; isnull(deputy.numWeeks,0) THEN intime.numWeeks else deputy.numWeeks END numWeeks,
			deputy.avHours,
			deputy.branchName
FROM		(

SELECT		NULL employeeId,
			cast(g.PK_SourceIdStr as varchar(50)) employeeRef,
			g.FirstName + ' ' + g.LastName employeeName ,
			cast(
				COUNT(DISTINCT 
				CASE
					WHEN DATEPART(DW,cast(h.RosterTo as date)) = DATEPART(DW,@altDate) THEN cast(h.RosterTo as date)
					ELSE d.PK_Date
				END
				) 
				as float) shiftCount,
			cast(DATEDIFF(DD, min(d.PK_Date),@altDate)/7 as float) numWeeks
FROM		dw.FACT_ROSTER h
INNER JOIN	dw.DIM_SECURITYGUARD g ON h.FK_DIM_SECURITYGUARD = g.SK_DIM_SECURITYGUARD
INNER JOIN	dw.DIM_DATE d ON h.FK_DIM_DATE = d.SK_DIM_DATE
INNER JOIN	dw.DIM_IT_STAFF_BRANCH sb ON g.SK_DIM_SECURITYGUARD = sb.FK_DIM_SECURITYGUARD AND isnull(sb.P_ExpiryDate,'2999-12-31') &gt;= CASE WHEN @altDate &gt; @altDate THEN @altDate ELSE @altDate END
INNER JOIN	dw.DIM_DEPOT dp ON sb.FK_DIM_DEPOT = dp.SK_DIM_DEPOT
LEFT JOIN	dw.DIM_BRANCH b on dp.FK_DIM_BRANCH = b.SK_DIM_BRANCH
WHERE		((h.RosterFrom &gt;= @altFromDate AND h.RosterFrom &lt; @altDate AND DATEPART(DW,h.RosterFrom) = DATEPART(DW,@altDate))
OR			(h.RosterTo &gt;= @altFromDate AND h.RosterTo &lt; @altDate AND DATEPART(DW,h.RosterTo) = DATEPART(DW,@altDate)))
AND			((b.[P_Branch Group] = 'Southern Region' AND h.RosterFrom &lt; @southCutover) OR (b.[P_Branch Group] != 'Southern Region' AND h.RosterFrom &lt; @northCutover))
GROUP BY	g.PK_SourceIdStr, g.FirstName + ' ' + g.LastName) intime

FULL OUTER JOIN (

SELECT			e.employeeId,
				ea.payrollId employeeRef,
				e.displayName employeeName,
				cast(
				COUNT(DISTINCT 
				CASE
					WHEN DATEPART(DW,cast(isnull(t.timesheetEnd, r.RosterEnd) as date)) = DATEPART(DW,@altDate) THEN cast(isnull(t.timesheetEnd, r.RosterEnd) as date)
					ELSE d.PK_Date
				END
				) 
				as float) shiftCount,
				cast(DATEDIFF(DD, min(d.PK_Date),@altDate)/7 as float) numWeeks,
				AVG(isnull(t.timesheetHours, r.Hours)) avHours,
				l1.Name branchName
FROM			staging.FCT_DEP_ROSTER r
FULL OUTER JOIN staging.FCT_DEP_Timesheet t ON r.Id = t.RosterId
INNER JOIN		dw.DIM_DATE d on isnull(cast(t.timesheetStart as date),r.RosterDate) = d.PK_Date
LEFT JOIN		staging.DEP_EmployeeAgreement ea On isnull(t.EmployeeId, r.EmployeeId) = ea.employeeId
INNER JOIN		staging.DEP_Employee e ON isnull(t.EmployeeId, r.EmployeeId) = e.employeeId
INNER JOIN		staging.DEP_EmployeeBranchLocation ebl ON e.employeeId = ebl.Id
LEFT JOIN		staging.DEP_Location l1 ON ebl.Company = l1.Id 
LEFT JOIN		staging.DEP_Location l2 ON l1.ParentId = l2.Id 
LEFT JOIN		staging.DEP_Location l3 ON l2.ParentId = l3.Id 
WHERE			((isnull(t.timesheetStart,r.RosterStart) &gt;= @altFromDate AND isnull(t.timesheetStart,r.RosterStart) &lt; @altDate AND DATEPART(DW,isnull(t.timesheetStart,r.RosterStart)) = DATEPART(DW,@altDate))
	OR				(isnull(t.timesheetEnd,r.RosterEnd) &gt;= @altFromDate AND isnull(t.timesheetEnd,r.RosterEnd) &lt; @altDate AND DATEPART(DW,isnull(t.timesheetEnd,r.RosterEnd)) = DATEPART(DW,@altDate)))
AND				((CASE WHEN l3.name like '%region' THEN l3.name WHEN l2.name like'%region' THEN l2.name ELSE l1.name END = 'South Region' AND isnull(t.timesheetStart,r.RosterStart) &gt;= @southCutover)
	OR				(CASE WHEN l3.name like '%region' THEN l3.name WHEN l2.name like'%region' THEN l2.name ELSE l1.name END != 'South Region' AND isnull(t.timesheetStart,r.RosterStart) &gt;= @northCutover))
AND				t.Discarded = 0

GROUP BY		e.employeeId, ea.payrollId, e.displayName, l1.Name
				) deputy ON intime.employeeRef = deputy.employeeRef

				) aqry ON mQry.employeeRef = aqry.employeeRef

FULL OUTER JOIN (

SELECT			e.employeeId,
				ea.payrollId employeeRef, cast(1 as bit) StatTaken, e.displayName employeeName
FROM			staging.FCT_DEP_TIMESHEET t
--FULL OUTER JOIN staging.FCT_DEP_LEAVELINE ll ON t.Id = ll.TimesheetId
LEFT JOIN		staging.DEP_EmployeeAgreement ea ON t.AgreementId = ea.agreementId -- isnull(t.AgreementId, ll.agreementId) = ea.agreementId
LEFT JOIN		staging.DEP_Employee e ON ea.employeeId = e.employeeId
WHERE			t.LeaveRuleId IN (12,26) --isnull(t.LeaveRuleId,ll.LeaveRuleId) IN (12, 26)  -- stat holiday types in Deputy
AND				cast(t.timesheetStart as date) = @mainDate --isnull(cast(t.timesheetStart as date), ll.LeaveDate) = @mainDate


				) mStat ON coalesce(mQry.employeeRef, aQry.employeeRef) = mStat.employeeRef

FULL OUTER JOIN (

SELECT			e.employeeId,
				ea.payrollId employeeRef, cast(1 as bit) StatTaken, e.displayName employeeName
FROM			staging.FCT_DEP_TIMESHEET t
--FULL OUTER JOIN staging.FCT_DEP_LEAVELINE ll ON t.Id = ll.TimesheetId
LEFT JOIN		staging.DEP_EmployeeAgreement ea ON t.AgreementId = ea.agreementId -- isnull(t.AgreementId, ll.agreementId) = ea.agreementId
LEFT JOIN		staging.DEP_Employee e ON ea.employeeId = e.employeeId
WHERE			t.LeaveRuleId IN (12,26) --isnull(t.LeaveRuleId,ll.LeaveRuleId) IN (12, 26)  -- stat holiday types in Deputy
AND				cast(t.timesheetStart as date) = @altDate --isnull(cast(t.timesheetStart as date), ll.LeaveDate) = @altDate


				) aStat ON coalesce(mQry.employeeRef, aQry.employeeRef, mStat.employeeRef) = aStat.employeeRef

--FULL OUTER JOIN (

--SELECT			e.employeeId,
--				ea.payrollId employeeRef,
--				e.displayName employeeName,
--				CASE
--					WHEN cast(t.timesheetStart as Date) = @mainDate AND cast (t.timesheetEnd as Date) = @mainDate THEN t.timesheetHours
--					WHEN cast(t.timesheetStart as Date) = @mainDate THEN cast(datediff(MI,t.timesheetStart,dateadd(dd,1,@mainDate)) as float) / 60
--					ELSE cast(datediff(MI,@mainDate,t.timesheetEnd) as float) / 60
--				END hoursWorked
--FROM			staging.FCT_DEP_TIMESHEET t
--INNER JOIN		staging.DEP_Employee e ON t.EmployeeId = e.employeeId
--LEFT JOIN		staging.DEP_EmployeeAgreement ea ON t.EmployeeId = ea.employeeId
--WHERE			((t.timesheetStart &gt;= @mainDate AND t.timesheetStart &lt; dateadd(DD,1,@mainDate))
--	OR				(t.timesheetEnd &gt;= @mainDate AND t.timesheetEnd &lt; dateadd(DD,1,@mainDate)))
--AND				t.LeaveRuleId is NULL
				
--				) mWorked ON coalesce(mQry.employeeRef, aQry.employeeRef, mStat.employeeRef, aStat.employeeRef) = mWorked.employeeRef

--FULL OUTER JOIN (

