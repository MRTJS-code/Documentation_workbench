          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{8D5FC3E1-EC64-4803-A0C3-D8A6A2D7D5E8}"
              SQLTask:SqlStatementSource="/** Build public holiday lookup table **/ -- add previous payroll alt credit tracking when public holiday is on a Monday&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@mainDate date = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@isNational bit = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@altDate date = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@phSK int = ?;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@eventId int = ?;&#xA;&#xA;DECLARE&#x9;&#x9;&#x9;&#x9;&#x9;@mainNextDay date = DATEADD(DD,1,@mainDate);&#xA;&#xA;SELECT&#x9;&#x9;&#x9;&#x9;&#x9;f.Id timesheetId,f.EmployeeId,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (f.timesheetStart &gt;= @mainDate AND f.timesheetStart &lt; @mainNextDay&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND (f.timesheetEnd &gt;= @mainDate AND f.timesheetEnd &lt; @mainNextDay)) THEN f.timesheetHours&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN f.timesheetStart &gt;= @mainDate AND f.timesheetStart &lt; @mainNextDay THEN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Cast(DATEDIFF(MI,f.timesheetStart,@mainNextDay) as float) / 60&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN f.timesheetEnd &gt;= @mainDate AND f.timesheetEnd &lt; @mainNextDay THEN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CAST(DATEDIFF(MI,@mainDate,f.timesheetEnd) as float) /60&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END phHours,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;w.mainWorkingDay,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;w.mainStatHolidayTaken&#xA;INTO&#x9;&#x9;&#x9;&#x9;&#x9;#phLookup&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL f&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;eda.LKP_WorkingDay w ON f.EmployeeId = w.employeeId&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;f.MDEventId = @eventId&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(f.timesheetHours,0) != 0&#xA;AND&#x9;(&#x9;&#x9;&#x9;&#x9;&#x9;f.timesheetStart &gt;= @mainDate AND f.timesheetStart &lt; @mainNextDay)&#xA;OR (&#x9;&#x9;&#x9;&#x9;&#x9;f.timesheetEnd &gt;= @mainDate AND f.timesheetEnd &lt; @mainNextDay);&#xA;&#xA;&#xA;/** Update Full Extract hours **/&#xA;UPDATE&#x9;&#x9;&#x9;&#x9;&#x9;d&#xA;SET&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.phHours = isnull(d.phHours,0) + p.phHours,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.timesheetHours = d.timesheetHours - p.phHours,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.phRef = @phSK&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL d&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;#phLookup p ON d.Id = p.timesheetId&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;p.phHours &gt; 0&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.LeaveRuleId is NULL&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.MDEventId = @eventId&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.payActivityCode NOT IN ('OD'); -- only split the main shift lines&#xA;&#xA;/** Update Full Extract alt day credit **/&#xA;UPDATE&#x9;&#x9;&#x9;&#x9;&#x9;d&#xA;SET&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.altCredit = 1,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.phRef = @phSK&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL d&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;#phLookup p ON d.Id = p.timesheetId&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;p.mainWorkingDay = 1&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.LeaveRuleId is NULL&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(d.altCredit,0) != 1&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.payActivityCode != 'OD'&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;p.mainStatHolidayTaken = 0&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.MDEventId = @eventId&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.payCentre IN ('Employee Timesheets', 'Agency Timesheets'); -- alt credit not required on subcontractor or internal records (non payroll)&#xA;&#xA;/** Remove alt credits if awarded for that holiday previous **/&#xA;UPDATE&#x9;&#x9;&#x9;&#x9;&#x9;d&#xA;SET&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.altCredit = 0&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL d&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;eda.LKP_PHTRACKER t ON d.EmployeeId = t.EmployeeId AND d.phRef = t.phRef AND d.altCredit = t.altCredit&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;d.altCredit = 1&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.phRef = @phSK&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.MDEventId = @eventId;&#xA;&#xA;/** Remove regional alt credits if employee already has this for another regional holiday **/&#xA;UPDATE&#x9;&#x9;&#x9;&#x9;&#x9;d&#xA;SET&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.altCredit = 0&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL d&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;eda.LKP_PHTRACKER t ON d.EmployeeId = t.EmployeeId AND t.isNational = 0 AND d.altCredit = t.altCredit&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;d.altCredit = 1&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.phRef = @phSK&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;d.MDEventId = @eventId&#xA;AND&#x9;&#x9;&#x9;&#x9;&#x9;@isNational = 0;&#xA;&#xA;/** check for same extract multiple alt day credits **/&#xA;WITH qry AS(&#xA;SELECT&#x9;&#x9;&#x9;&#x9;&#x9;f.Id, f.EmployeeId, ROW_NUMBER() OVER (PARTITION BY f.employeeId ORDER BY f.employeeId, f.Id) RowNum&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL f&#xA;WHERE&#x9;&#x9;&#x9;&#x9;&#x9;f.phRef = @phSK AND f.altCredit = 1 AND f.MDEventId = @eventId)&#xA;UPDATE&#x9;&#x9;&#x9;&#x9;&#x9;f&#xA;SET&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;f.altCredit = 0&#xA;FROM&#x9;&#x9;&#x9;&#x9;&#x9;qry&#xA;INNER JOIN&#x9;&#x9;&#x9;&#x9;eda.EXT_DEP_FULL f on qry.Id = f.Id&#xA;WHERE RowNum&#x9;&#x9;&#x9;&gt; 1;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ParameterBinding
