        <DTS:Executable
          DTS:refId="Package\Foreach Loop Container\Split and Alt Credit Code"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{E007BA3A-B3E3-4474-AC78-A52F4A81305A}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
